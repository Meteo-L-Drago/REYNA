{
  "expo": {
    "name": "REYNA",
    "slug": "REYNA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "REYNA nutzt die Foto-Bibliothek f端r Produktbilder.",
          "cameraPermission": "REYNA nutzt die Kamera f端r Produktbilder."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reyna.app",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "REYNA nutzt die Foto-Bibliothek f端r Produktbilder.",
        "NSCameraUsageDescription": "REYNA nutzt die Kamera f端r Produktbilder."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.reyna.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
